      "cell_type": "code",
      "source": [
        "import requests\n",
        "import json\n",
        "import os\n",
        "\n",
        "def obter_dados_dengue(municipio, estado, ano):\n",
        "  \"\"\"Obtém dados de dengue de uma API.\n",
        "\n",
        "  Args:\n",
        "    municipio: O município de interesse.\n",
        "    estado: O estado de interesse.\n",
        "    ano: O ano de interesse.\n",
        "\n",
        "  Returns:\n",
        "    Uma lista de casos de dengue ou None em caso de erro.\n",
        "  \"\"\"\n",
        "  api_key = os.environ.get(\"API_KEY\")\n",
        "  endpoint = \"https://api.example.com/dengue\"\n",
        "\n",
        "  headers = {\n",
        "      \"Authorization\": f\"Bearer {api_key}\",\n",
        "      \"Content-Type\": \"application/json\",\n",
        "  }\n",
        "\n",
        "  params = {\n",
        "      \"municipio\": municipio,\n",
        "      \"estado\": estado,\n",
        "      \"ano\": ano,\n",
        "  }\n",
        "\n",
        "  response = requests.get(endpoint, headers=headers, params=params)\n",
        "\n",
        "  if response.status_code == 200:\n",
        "    data = json.loads(response.text)\n",
        "\n",
        "    if \"casos\" in data:\n",
        "      return data[\"casos\"]\n",
        "    else:\n",
        "      print(\"Erro: Estrutura da resposta da API inválida.\")\n",
        "      return None\n",
        "  else:\n",
        "    print(f\"Erro na requisição: {response.status_code} - {response.text}\")\n",
        "    return None\n",
        "\n",
        "\n",
        "if __name__ == \"__main__\":\n",
        "  casos_dengue = obter_dados_dengue(\"Caçapava\", \"SP\", 2024)\n",
        "\n",
        "  if casos_dengue:\n",
        "    for caso in casos_dengue:\n",
        "      print(f\"Data: {caso.get('data', 'N/A')}\")\n",
        "      print(f\"Local: {caso.get('municipio', 'N/A')}, {caso.get('estado', 'N/A')}\")\n",
        "      print(f\"Tipo: {caso.get('tipo', 'N/A')}\")\n",
        "      print(\"-\" * 20)"
      ],
